<h1>
	My Projects
</h1>

<div class="row">
<div class="span6 offset3">
<%= form_for(@project) do |f| %>
<%= render 'shared/error_messages', object: f.object %>
<%= f.label :name %>
<%= f.text_field :name %>
<%= f.label :desc %>
<%= f.text_field :desc %>
<%= f.submit "Create new Project", class: "btn btn-large btn-primary" %>
<% end %>
</div>
</div>